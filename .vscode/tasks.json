{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build (batch)",
      "type": "shell",
      "command": "C:\\Windows\\System32\\cmd.exe",
      "args": ["/d", "/c", "\"${workspaceFolder}\\build_msvc.bat\""],
      "options": { "cwd": "${workspaceFolder}" },
      "group": { "kind": "build", "isDefault": true },
      "problemMatcher": ["$msCompile"]
    },
    {
      "label": "build (release)",
      "type": "shell",
      "command": "C:\\Windows\\System32\\cmd.exe",
      "args": ["/d","/c","\"${workspaceFolder}\\build_msvc.bat\"","release"],
      "options": { "cwd": "${workspaceFolder}" },
      "group": "build",
      "problemMatcher": ["$msCompile"]
    },
    {
      "label": "clean",
      "type": "shell",
      "command": "C:\\Windows\\System32\\cmd.exe",
      "args": ["/d","/c","\"${workspaceFolder}\\build_msvc.bat\"","clean"],
      "options": { "cwd": "${workspaceFolder}" },
      "problemMatcher": []
    }
  ]
}